{
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 250,
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreUrls": true
      }
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "log"
        ]
      }
    ],
    "linebreak-style": ["error", "windows"]
  }
}
